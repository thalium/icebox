+bool FDPVBOX_Reboot(void *pUserHandle)
+{    
+    Log1("REBOOT\n");
+    FDPVBOX_USERHANDLE_T* myVBOXHandle = (FDPVBOX_USERHANDLE_T*)pUserHandle;
+    PUVM pUVM = myVBOXHandle->pUVM;
+
+    PVMCPU pVCpu = VMMR3GetCpuByIdU(pUVM, 0);
