+bool IsOneCPURunning(PUVM pUVM)
+{
+    for(uint32_t i=0; i<VMR3GetCPUCount(pUVM); i++){
+        if(!(pUVM->pVM->aCpus[i].mystate.s.u8StateBitmap & FDP_STATE_PAUSED)){
+            return true;
+        }
+    }
+    return false;
+}
