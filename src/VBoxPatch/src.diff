+VMMDECL(int) VMR3Break(PUVM pUVM)
+{
+    RTSpinlockAcquire(pUVM->pVM->mystate.s.CpuLock);
+
+    VMR3BreakNoLock(pUVM);
+    
+    RTSpinlockRelease(pUVM->pVM->mystate.s.CpuLock);
+    return 0;
+}
