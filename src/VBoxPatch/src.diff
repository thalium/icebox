+    FDPServerInterface.pfnResume =  &FDPVBOX_Resume;
+    FDPServerInterface.pfnSingleStep =  &FDPVBOX_singleStep;
+    FDPServerInterface.pfnPause =  &FDPVBOX_Pause;
+    FDPServerInterface.pfnReadMsr =  &FDPVBOX_readMsr;
