	$(QUIET)$(VBOX_NM) $^  2> /dev/null \
		| $(SED) -n \
			-e '/^ *U .*/!d' \
			-e 's/ *U //' \
			\
			-e '/^g_VM\>/d'\
			-e '/^g_CPUM\>/d'\
			-e '/^g_Logger\>/d'\
