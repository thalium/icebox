DLLS += wined3d
wined3d_TEMPLATE     = VBoxGuestWineDLL
wined3d_SDKS         = ReorderCompilerIncs $(VBOX_WINDDK_GST_WLH)
wined3d_DEFS         = \
	_USE_MATH_DEFINES \
	VBOX_USING_WINDDK_W7_OR_LATER \
	VBOX_WINE_WITHOUT_LIBWINE \
	VBOX_WINE_WITH_SINGLE_CONTEXT \
	VBOX_WINE_WITH_IPRT
