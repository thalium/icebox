SUB_DEPTH = ../../..
include $(KBUILD_PATH)/subheader.kmk

LIBRARIES += VBox-libpng
VBox-libpng_TEMPLATE = VBoxR3DllNonPedantic
VBox-libpng_SDKS = VBOX_ZLIB
VBox-libpng_CFLAGS.win = -wd4310 # pngrutil.c(1268) : warning C4310: cast truncates constant value
VBox-libpng_SOURCES = \
	png.c \
	pngset.c \
	pngget.c \
	pngrutil.c \
	pngtrans.c \
	pngwutil.c \
	pngread.c \
	pngrio.c \
	pngwio.c \
	pngwrite.c \
	pngrtran.c \
	pngwtran.c \
	pngmem.c \
	pngerror.c \
	pngpread.c

include $(FILE_KBUILD_SUB_FOOTER)

